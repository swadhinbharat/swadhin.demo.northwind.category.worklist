{
    "name": "swadhin.demo.northwind.category.worklist",
    "version": "0.0.1",
    "private": true,
    "sapux": true,
    "description": "A Fiori application.",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "start": "fiori run --open 'test/flpSandbox.html#swadhindemonorthwindcategoryworklist-tile'",
        "start-local": "fiori run --config ./ui5-local.yaml --open 'test/flpSandboxMockServer.html#swadhindemonorthwindcategoryworklist-tile'",
        "start-noflp": "fiori run --open 'index.html'",
        "build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "start-mock": "fiori run --open 'test/flpSandboxMockServer.html#swadhindemonorthwindcategoryworklist-tile'",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml"
    },
    "devDependencies": {
        "@sap/ux-specification": "1.100.3",
        "@sap/ux-ui5-tooling": "1.6.0",
        "@ui5/cli": "2.14.9",
        "@ui5/fs": "2.0.6",
        "@ui5/logger": "2.0.1",
        "rimraf": "3.0.2"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling"
        ]
    }
}
